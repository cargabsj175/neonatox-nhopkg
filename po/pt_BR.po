#: ../src/nhopkg.in:197
msgid "Examples:"
msgstr "Exemplos:"

#: ../src/nhopkg.in:198
msgid " * Install binary package."
msgstr " * Instalar o pacote binário."

#: ../src/nhopkg.in:200
msgid " * Show files on package."
msgstr "  * Mostrar arquivos no pacote."

#: ../src/nhopkg.in:203
msgid "Commands:"
msgstr "Comandos:"

#: ../src/nhopkg.in:204
msgid " -i, --install\tInstalls binary package."
msgstr " -i, --install\tInstala o pacote binário."

#: ../src/nhopkg.in:205
msgid " -S, --super-install\tInstalls binary package form repository."
msgstr " -S, --super-install\tInstala o pacote binário do repositório."

#: ../src/nhopkg.in:206
msgid " -d, --dios\tSame as -S, --super-install."
msgstr " -d, --dios\tIgual a -S, --super-install."

#: ../src/nhopkg.in:207
msgid " -r, --remove\tRemoves package installed by Nhopkg."
msgstr " -r, --remove\tRemove o pacote instalado pelo Nhopkg."

#: ../src/nhopkg.in:208
msgid " -B, --backup\tCreates binary package from installed package."
msgstr " -B, --backup\tCria pacote binário a partir do pacote instalado."

#: ../src/nhopkg.in:209
msgid " -b, --build\tCompiles and installs source package."
msgstr " -b, --build\tCompila e instala o pacote fonte."

#: ../src/nhopkg.in:210
msgid " -k, --check\tChecks if installed package is correct."
msgstr " -k, --check\tVerifica se o pacote instalado está correto."

#: ../src/nhopkg.in:211
msgid " -c, --create-source\tCreates source package from tarball."
msgstr " -c, --create-source\tCria um pacote fonte a partir do tarball."

#: ../src/nhopkg.in:212
msgid " -A, --auto-package\tCreates automatically source package from tarball."
msgstr " -A, --auto-package\tCria automaticamente um pacote fonte a partir do tarball."

#: ../src/nhopkg.in:213
msgid " -z, --tgz2nho\tConverts .tgz package to .nho package."
msgstr " -z, --tgz2nho\tConverte o pacote .tgz para .nho."

#: ../src/nhopkg.in:214
msgid " -g, --create-repo\tCreates software repository on selected directory."
msgstr " -g, --create-repo\tCria um repositório de software no diretório selecionado."

#: ../src/nhopkg.in:215
msgid " -U, --update\tUpdate repository database."
msgstr " -U, --update\tAtualiza o banco de dados do repositório."

#: ../src/nhopkg.in:216
msgid " -e, --clean\tRemove cached data."
msgstr " -e, --clean\tRemove os dados em cache."

#: ../src/nhopkg.in:217
msgid " -s, --search\tSearch for package and its details."
msgstr " -s, --search\tBusca por pacotes e seus detalhes."

#: ../src/nhopkg.in:218
msgid " -t, --list-repo\tLists packages on repository and their details."
msgstr " -t, --list-repo\tLista pacotes no repositório e seus detalhes."

#: ../src/nhopkg.in:219
msgid " -l, --list\tLists installed packages."
msgstr " -l, --list\tLista pacotes instalados."

#: ../src/nhopkg.in:220
msgid " -n, --info\tShows package details."
msgstr " -n, --info\tExibe detalhes do pacote."

#: ../src/nhopkg.in:221
msgid " -w, --show\tShows package files."
msgstr " -w, --show\tExibe os arquivos do pacote."

#: ../src/nhopkg.in:222
msgid " -u, --update-db\tUpdate Nhopkg database."
msgstr " -u, --update-db\tAtualiza o banco de dados do Nhopkg."

#: ../src/nhopkg.in:223
msgid " --license\tShows Nhopkg license."
msgstr " --license\tExibe a licença do Nhopkg."

#: ../src/nhopkg.in:224
msgid " --license-all\tShows complete Nhopkg license."
msgstr " --license-all\tExibe a licença completa do Nhopkg."

#: ../src/nhopkg.in:225
msgid " --version\tShows Nhopkg version."
msgstr " --version\tExibe a versão do Nhopkg."

#: ../src/nhopkg.in:226
msgid " -h, --help\tShows Nhopkg help pages."
msgstr " -h, --help\tExibe as páginas de ajuda do Nhopkg."

#: ../src/nhopkg.in:228
msgid "Options:"
msgstr "Opções:"

#: ../src/nhopkg.in:229
msgid " -R, --recursive\tNhopkg works recursively."
msgstr " -R, --recursive\tNhopkg funciona recursivamente."

#: ../src/nhopkg.in:230
msgid " --root\tSets install root."
msgstr " --root\tDefine a raiz de instalação."

#: ../src/nhopkg.in:231
msgid " --no-check-deps\tDon't check dependencies."
msgstr " --no-check-deps\tNão verifica dependências."

#: ../src/nhopkg.in:232
msgid " --no-check-arch\tDon't check architecture."
msgstr " --no-check-arch\tNão verifica a arquitetura."

#: ../src/nhopkg.in:233
msgid " --no-check-md5\tDon't check MD5 Sum."
msgstr " --no-check-md5\tNão verifica o MD5."

#: ../src/nhopkg.in:234
msgid " --force-check-deps\tForce check dependencies."
msgstr " --force-check-deps\tVerificação forçada de dependências."

#: ../src/nhopkg.in:235
msgid " --force-check-arch\tForce check architecture."
msgstr " --force-check-arch\tVerificação forçada da arquitetura."

#: ../src/nhopkg.in:236
msgid " --force-check-md5\tForce check MD5 Sum."
msgstr " --force-check-md5\tVerificação forçada do MD5."

#: ../src/nhopkg.in:237
msgid " --purge\tRemove inverse dependencies of package."
msgstr " --purge\tRemove dependências inversas do pacote."

#: ../src/nhopkg.in:238
msgid " -o, --output\tWrite an output log."
msgstr " -o, --output\tEscreve um log de saída."

#: ../src/nhopkg.in:239
msgid " -v, --verbose\tVerbose operations."
msgstr " -v, --verbose\tOperações detalhadas."

#: ../src/nhopkg.in:240
msgid " --\tStop parssing arguments."
msgstr " --\tInterrompe a análise dos argumentos."

