#: ../src/nhopkg.in:197
msgid "Examples:"
msgstr "Exemples :"

#: ../src/nhopkg.in:198
msgid " * Install binary package."
msgstr "* Installer le paquet binaire."

#: ../src/nhopkg.in:200
msgid " * Show files on package."
msgstr " * Montrer les fichiers du paquet."

#: ../src/nhopkg.in:203
msgid "Commands:"
msgstr "Commandes :"

#: ../src/nhopkg.in:204
msgid " -i, --install\tInstalls binary package."
msgstr " -i, --install\tInstaller le paquet binaire."

#: ../src/nhopkg.in:205
msgid " -S, --super-install\tInstalls binary package form repository."
msgstr " -S, --super-install\tInstaller le paquet d'un depôt."

#: ../src/nhopkg.in:206
msgid " -d, --dios\tSame as -S, --super-install."
msgstr " -d, --dios\tLa même chise que -S, --super-install."

#: ../src/nhopkg.in:207
msgid " -r, --remove\tRemoves package installed by Nhopkg."
msgstr " -r, --remove\tDésinstiler un paquet installé par Nhopkg"

#: ../src/nhopkg.in:208
msgid " -B, --backup\tCreates binary package from installed package."
msgstr " -B, --backup\tCréer un paquet binaire d'après un paquet déjà installé."

#: ../src/nhopkg.in:209
msgid " -b, --build\tCompiles and installs source package."
msgstr " -b, --build\tCompiler et installer un paquet source."

#: ../src/nhopkg.in:210
msgid " -k, --check\tChecks if installed package is correct."
msgstr " -k, --check\tVérificationif de l'installation."

#: ../src/nhopkg.in:211
msgid " -c, --create-source\tCreates source package from tarball."
msgstr " -c, --create-source\tCréer un paquet source d'après une archive TAR."

#: ../src/nhopkg.in:212
msgid " -A, --auto-package\tCreates automatically source package from tarball."
msgstr " -A, --auto-package\tCréer automatiquement un paquet source d'après une archive TAR."

#: ../src/nhopkg.in:213
msgid " -z, --tgz2nho\tConverts .tgz package to .nho package."
msgstr " -z, --tgz2nho\tConvertit .tgz paquets en .nho paquets."

#: ../src/nhopkg.in:214
msgid " -g, --create-repo\tCreates software repository on selected directory."
msgstr " -g, --create-repo\tCréer un dépôt."

#: ../src/nhopkg.in:215
msgid " -U, --update\tUpdate repository database."
msgstr " -U, --update\tMise à jour de la base de données."

#: ../src/nhopkg.in:216
msgid " -e, --clean\tRemove cached data."
msgstr " -e, --clean\tEffacer les données en caché."

#: ../src/nhopkg.in:217
msgid " -s, --search\tSearch for package and its details."
msgstr " -s, --search\tChercher paquets et détails."

#: ../src/nhopkg.in:218
msgid " -t, --list-repo\tLists packages on repository and their details."
msgstr " -t, --list-repo\tLister les paquets du dépôt et les détails."

#: ../src/nhopkg.in:219
msgid " -l, --list\tLists installed packages."
msgstr " -l, --list\tLister les paquets installés."

#: ../src/nhopkg.in:220
msgid " -n, --info\tShows package details."
msgstr " -n, --info\tAfficher les détails."

#: ../src/nhopkg.in:221
msgid " -w, --show\tShows package files."
msgstr " -w, --show\tMontrer l'information du paquet."

#: ../src/nhopkg.in:222
msgid " -u, --update-db\tUpdate Nhopkg database."
msgstr " -u, --update-db\tMise à jour de la base de données."

#: ../src/nhopkg.in:223
msgid " --license\tShows Nhopkg license."
msgstr " --license\tAffiche l'information de licence."

#: ../src/nhopkg.in:224
msgid " --license-all\tShows complete Nhopkg license."
msgstr " --license-all\tAffiche l'information de licence complète."

#: ../src/nhopkg.in:225
msgid " --version\tShows Nhopkg version."
msgstr " --version\tAffiche l'information de version."

#: ../src/nhopkg.in:226
msgid " -h, --help\tShows Nhopkg help pages."
msgstr " -h, --help\tAfficher l'aide."

#: ../src/nhopkg.in:228
msgid "Options:"
msgstr "Options :"

#: ../src/nhopkg.in:229
msgid " -R, --recursive\tNhopkg works recursively."
msgstr " -R, --recursive\tNhopkg travaille récursivement."

#: ../src/nhopkg.in:230
msgid " --root\tSets install root."
msgstr " --root\tRacine d'installation."

#: ../src/nhopkg.in:231
msgid " --no-check-deps\tDon't check dependencies."
msgstr " --no-check-deps\tNe pas vérifier les dépendances."

#: ../src/nhopkg.in:232
msgid " --no-check-arch\tDon't check architecture."
msgstr " --no-check-arch\tNe pas vérifier l'architecture."

#: ../src/nhopkg.in:233
msgid " --no-check-md5\tDon't check MD5 Sum."
msgstr " --no-check-md5\tNe pas vérifier MD5Sum."

#: ../src/nhopkg.in:234
msgid " --force-check-deps\tForce check dependencies."
msgstr " --force-check-deps\tForcer la vérification de dépendances."

#: ../src/nhopkg.in:235
msgid " --force-check-arch\tForce check architecture."
msgstr " --force-check-arch\tForcer la vérification de l'architecture."

#: ../src/nhopkg.in:236
msgid " --force-check-md5\tForce check MD5 Sum."
msgstr " --force-check-md5\tForcer la vérification MD5Sum"

#: ../src/nhopkg.in:237
msgid " --purge\tRemove inverse dependencies of package."
msgstr " --purge\tSuppression inverse des dépendances du paquet."

#: ../src/nhopkg.in:238
msgid " -o, --output\tWrite an output log."
msgstr " -o, --output\tÉcrire un journal."

#: ../src/nhopkg.in:239
msgid " -v, --verbose\tVerbose operations."
msgstr " -v, --verbose\tMode bavard."

#: ../src/nhopkg.in:240
msgid " --\tStop parssing arguments."
msgstr " --\tArrêter l'analyse des arguments."

