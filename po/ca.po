#: src/nhopkg.in:38 src/nhopkg.in:42
msgid " * Unable to locate Nhopkg configuration file!"
msgstr " * No s'ha pogut trobar l'arxiu de configuració de Nhopkg!"

#: src/nhopkg.in:39
msgid " - Loading default Nhopkg configuration file..."
msgstr " - Carregant l'arxiu de configureció per defecte de Nhopkg..."

#: src/nhopkg.in:295
msgid "Great Work your LFS! ;-)"
msgstr "Bona feina pel teu LFS! ;-)"

#: src/nhopkg.in:297
msgid "Nhopkg version"
msgstr "versió del Nhopkg"

#: src/nhopkg.in:312
msgid "Usage: nhopkg [command] [options] package"
msgstr "Ús: nhopkg [command] [options] package"

#: src/nhopkg.in:313 src/nhopkg.in:326
msgid "To see the help: nhopkg --help"
msgstr "Per veure l'ajuda: nhopkg [command] [options] package"

#: src/nhopkg.in:325
msgid "Usage: sudo nhopkg [command] [options] package"
msgstr "Ús: sudo nhopkg [command] [options] package"

#: src/nhopkg.in:340
msgid "This program comes with ABSOLUTELY NO WARRANTY."
msgstr "Aquest programa no preveu CAP GARANTIA EN ABSOLUT."

#: src/nhopkg.in:341
msgid ""
"This is free software, and you are welcome to redistribute it under certain "
"conditions; for details type nhopkg --license-all."
msgstr ""
"Això és programari lliure, i ets benvingut a distribuir-lo sota certes "
"condicions; per a més detalls escriu: nhopkg --license-all"

#: src/nhopkg.in:355
msgid "Examples:"
msgstr "Exemples:"

#: src/nhopkg.in:356
msgid " * Install binary package"
msgstr " * Instal·lar paquet binari"

#: src/nhopkg.in:358
msgid " * Show files on package."
msgstr " * Mostrar fitxers del paquet."

#: src/nhopkg.in:361
msgid "Commands:"
msgstr "Ordres:"

#: src/nhopkg.in:362
msgid  " -A, --auto-package\tCreates automatically source package from tarball."
msgstr  " -A, --auto-package	Crea automàticament un paquet amb el codi font des del tarball."

#: src/nhopkg.in:363
msgid " -B, --backup\tCreates binary package from installed package."
msgstr " -B, --backup\tCrea un paquet binari a partir del paquet instal·lat."

#: src/nhopkg.in:364
msgid " -S, --super-install\tInstalls binary package form repository."
msgstr " -S, --super-install\tInstal·la el paquet binari des del dipòsit."

#: src/nhopkg.in:365
msgid " -U, --update\tUpdate repository database."
msgstr " -U, --update\tActualitza la base de dades del dipòsit."

#: src/nhopkg.in:367
msgid " -b, --build\tCompiles and installs source package."
msgstr " -b, --build\tCompila i instal·la el paquet amb el codi font."

#: src/nhopkg.in:368
msgid " -c, --create-source\tCreates source package from tarball."
msgstr ""
" -c, --create-source\tCrea un paquet amb el codi font des d'un tarball."

#: src/nhopkg.in:369
msgid " -d, --dios\tSame as -S, --super-install."
msgstr " -d, --dios\tÉs igual que -S, --super-install."

#: src/nhopkg.in:370
msgid " -e, --clean\tRemove cached data."
msgstr " -e, --clean\tNeteja les dades de la memòria cau."

#: src/nhopkg.in:371
msgid " -g, --create-repo\tCreates software repository on selected directory."
msgstr ""
" -g, --create-repo\tCrea un dipòsit de programari en el directori indicat."

#: src/nhopkg.in:372
msgid " -i, --install\tInstalls binary package."
msgstr " -i, --install\tInstal·la el paquet binari."

#: src/nhopkg.in:373
msgid " -k, --check\tChecks if installed package is correct."
msgstr " -k, --check\tComprova si el paquet instal·lat és correcte."

#: src/nhopkg.in:374
msgid " -l, --list\tLists installed packages."
msgstr " -l, --list\tLlistar paquets instal·lats."

#: src/nhopkg.in:375
msgid " -n, --info\tShows package details."
msgstr " -n, --info\tMostrar informació del paquet."

#: src/nhopkg.in:376
msgid " -r, --remove\tRemoves package installed by Nhopkg."
msgstr " -r, --remove\tEsborra el paquet instal·lat per Nhopkg."

#: src/nhopkg.in:377
msgid " -s, --search\tSearch for package and its details."
msgstr " -s, --search\tBuscar un paquet i la seva informació."

#: src/nhopkg.in:378
msgid " -t, --list-repo\tLists packages on repository and their details."
msgstr " -t, --list-repo\tLlistar paquets en el dipòsit i la seva informació."

#: src/nhopkg.in:379
msgid " -u, --update-db\tUpdate Nhopkg database."
msgstr " -u, --update-db\tActualitzar base de dades de Nhopkg."

#: src/nhopkg.in:380
msgid " -w, --show\tShows package files."
msgstr " -w, --show\tMostra fitxers del paquet."

#: src/nhopkg.in:381
msgid " -x, --update-shooters\tUpdates all system data and schemas."
msgstr ""
"-x, --update-shooters\tActualitza totes les dades i esquemes del sistema."

#: src/nhopkg.in:382
msgid " -z, --tgz2nho\tConverts .tgz package to .nho package."
msgstr " -z, --tgz2nho\tConverteix un paquet .tgz a un paquet .nho."

#: src/nhopkg.in:383
msgid " --license-all\tShows complete Nhopkg license."
msgstr " --license-all\tMostrar llicència del Nhopkg completa."

#: src/nhopkg.in:384
msgid " --license\tShows Nhopkg license."
msgstr " --license\tMostrar llicència del Nhopkg."

#: src/nhopkg.in:385
msgid " --version\tShows Nhopkg version."
msgstr " --version\tMostrar versió del Nhopkg."

#: src/nhopkg.in:386
msgid " -h, --help\tShows this help page."
msgstr " -h, --help\tMostrar pàgines d'ajuda de Nhopkg."

#: src/nhopkg.in:388
msgid "Options:"
msgstr "Opcions:"

#: src/nhopkg.in:389
msgid " --force-check-arch\tForce check architecture."
msgstr ""
" --force-check-arch\tForçar el revisar l'arquitectura (amd64, i686,...)."

#: src/nhopkg.in:390
msgid " --force-check-deps\tForce check dependencies."
msgstr " --force-check-deps\tForçar el revisar les dependències."

#: src/nhopkg.in:391
msgid " --force-check-sha256\tForce check SHA256 Sum."
msgstr " --force-check-md5\tForçar el revisar el MD5 Sum."

#: src/nhopkg.in:392
msgid " --no-check-arch\tDon't check architecture."
msgstr " --no-check-arch\tNo revisis l'arquitectura (amd64, i686,...)."

#: src/nhopkg.in:393
msgid " --no-check-deps\tDon't check dependencies."
msgstr " --no-check-deps\tNo verifiquis les dependències."

#: src/nhopkg.in:394
msgid " --no-check-sha256\tDon't check SHA256 Sum."
msgstr " --no-check-md5\tNo revisar el MD5 Sum."

#: src/nhopkg.in:395
msgid " --purge\tRemove inverse dependencies of package."
msgstr " --purge\tEliminar les dependències inverses del paquet."

#: src/nhopkg.in:396
msgid " --root\tSets install root."
msgstr " --root\tSelecciona el directori d'instal·lació."

#: src/nhopkg.in:397
msgid " -R, --recursive\tNhopkg works recursively."
msgstr " -R, --recursive\tNhopkg treballa recursivament."

#: src/nhopkg.in:398
msgid " -o, --output\tWrite an output log."
msgstr " -o, --output\tEscriure un registre del procés."

#: src/nhopkg.in:399
msgid " -v, --verbose\tVerbose operations."
msgstr " -v, --verbose\tOperacions verboses."

#: src/nhopkg.in:400
msgid " --\tStop parssing arguments."
msgstr " --\tNo obté més arguments."

#: src/nhopkg.in:416
msgid "Nhopkg is already in use..."
msgstr "Nhopkg segueix estan en ús..."

#: src/nhopkg.in:421
msgid "Exist an older lock file, but seems that is not in use..."
msgstr ""
"Existeix un arxiu lock més antic, però no semblar que s'estigui utilitzant..."

#: src/nhopkg.in:427
msgid " * You can't rewrite lock file."
msgstr " * Pots sobreescriure l'arxiu lock."

#: src/nhopkg.in:470
msgid "To run Nhopkg you need have root powers."
msgstr "Per fer funcionar el Nhopkg necessites tenir superpoders de root."

