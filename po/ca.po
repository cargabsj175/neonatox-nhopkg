msgid " -S, --super-install\tInstalls binary package form repository."
msgstr " -S, --super-install\tInstal·la el paquet binari des del dipòsit."

#: ../src/nhopkg.in:206
msgid " -d, --dios\tSame as -S, --super-install."
msgstr " -d, --dios\tÉs igual que -S, --super-install."

#: ../src/nhopkg.in:207
msgid " -r, --remove\tRemoves package installed by Nhopkg."
msgstr " -r, --remove\tEsborra el paquet instal·lat per Nhopkg."

#: ../src/nhopkg.in:208
msgid " -B, --backup\tCreates binary package from installed package."
msgstr " -B, --backup\tCrea un paquet binari a partir del paquet instal·lat."

#: ../src/nhopkg.in:209
msgid " -b, --build\tCompiles and installs source package."
msgstr " -b, --build\tCompila i instal·la el paquet amb el codi font."

#: ../src/nhopkg.in:210
msgid " -k, --check\tChecks if installed package is correct."
msgstr " -k, --check\tComprova si el paquet instal·lat és correcte."

#: ../src/nhopkg.in:211
msgid " -c, --create-source\tCreates source package from tarball."
msgstr ""
" -c, --create-source\tCrea un paquet amb el codi font des d'un tarball."

#: ../src/nhopkg.in:212
msgid " -A, --auto-package\tCreates automatically source package from tarball."
msgstr ""
" -A, --auto-package\tCrea automàticament un paquet amb el codi font des del "
"tarball."

#: ../src/nhopkg.in:213
msgid " -z, --tgz2nho\tConverts .tgz package to .nho package."
msgstr " -z, --tgz2nho\tConverteix un paquet .tgz a un paquet .nho."

#: ../src/nhopkg.in:214
msgid " -g, --create-repo\tCreates software repository on selected directory."
msgstr ""
" -g, --create-repo\tCrea un dipòsit de programari en el directori indicat."

#: ../src/nhopkg.in:215
msgid " -U, --update\tUpdate repository database."
msgstr " -U, --update\tActualitza la base de dades del dipòsit."

#: ../src/nhopkg.in:216
msgid " -e, --clean\tRemove cached data."
msgstr " -e, --clean\tNeteja les dades de la memòria cau."

#: ../src/nhopkg.in:217
msgid " -s, --search\tSearch for package and its details."
msgstr " -s, --search\tBuscar un paquet i la seva informació."

#: ../src/nhopkg.in:218
msgid " -t, --list-repo\tLists packages on repository and their details."
msgstr " -t, --list-repo\tLlistar paquets en el dipòsit i la seva informació."

#: ../src/nhopkg.in:219
msgid " -l, --list\tLists installed packages."
msgstr " -l, --list\tLlistar paquets instal·lats."

#: ../src/nhopkg.in:220
msgid " -n, --info\tShows package details."
msgstr " -n, --info\tMostrar informació del paquet."

#: ../src/nhopkg.in:221
msgid " -w, --show\tShows package files."
msgstr " -w, --show\tMostra fitxers del paquet."

#: ../src/nhopkg.in:222
msgid " -u, --update-db\tUpdate Nhopkg database."
msgstr " -u, --update-db\tActualitzar base de dades de Nhopkg."

#: ../src/nhopkg.in:223
msgid " --license\tShows Nhopkg license."
msgstr " --license\tMostrar llicència del Nhopkg."

