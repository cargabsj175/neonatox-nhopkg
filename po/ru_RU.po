#: src/nhopkg.in:38 src/nhopkg.in:42
msgid " * Unable to locate Nhopkg configuration file!"
msgstr "* Не могу найти конфигурационный файл Nhopkg!"

#: src/nhopkg.in:39
msgid " - Loading default Nhopkg configuration file..."
msgstr " - Загружается конфигурационный файл Nhopkg по умолчанию..."

#: src/nhopkg.in:295
msgid "Great Work your LFS! ;-)"
msgstr "Прекрасная работа, LFS ;-)"

#: src/nhopkg.in:297
msgid "Nhopkg version"
msgstr "Версия Nhopkg"

#: src/nhopkg.in:312
msgid "Usage: nhopkg [command] [options] package"
msgstr "Использование: nhopkg [command] [options] package"

#: src/nhopkg.in:313 src/nhopkg.in:326
msgid "To see the help: nhopkg --help"
msgstr "Чтобы посмотреть помощь: nhopkg --help"

#: src/nhopkg.in:325
msgid "Usage: sudo nhopkg [command] [options] package"
msgstr "Использование: sudo nhopkg [command] [options] package"

#: src/nhopkg.in:340
msgid "This program comes with ABSOLUTELY NO WARRANTY."
msgstr "Данная программа предоставляется без ВСЯКОЙ ГАРАНТИИ."

#: src/nhopkg.in:341
msgid ""
"This is free software, and you are welcome to redistribute it under certain "
"conditions; for details type nhopkg --license-all."
msgstr ""
"Это — свободное ПО, и вы вольны раздавать его с определенными условиями; "
"ради деталей выполните nhopkg --license-all."

#: src/nhopkg.in:355
msgid "Examples:"
msgstr "Примеры:"

#: src/nhopkg.in:356
msgid " * Install binary package"
msgstr " * Установить бинарный пакет"

#: src/nhopkg.in:358
msgid " * Show files on package."
msgstr " * Показать файл пакета."

#: src/nhopkg.in:361
msgid "Commands:"
msgstr "Команды:"

#: src/nhopkg.in:362
msgid " -A, --auto-package\tCreates automatically source package from tarball."
msgstr ""
" -A, --auto-package\tСоздать автоматически пакет исходников из  tarball'а."

#: src/nhopkg.in:363
msgid " -B, --backup\tCreates binary package from installed package."
msgstr " -B, --backup\tСоздать бинарный пакет из установленного пакета."

#: src/nhopkg.in:364
msgid " -S, --super-install\tInstalls binary package form repository."
msgstr " -S, --super-install\tУстановить бинарный пакет из репозитория."

#: src/nhopkg.in:365
msgid " -U, --update\tUpdate repository database."
msgstr " -U, --update\tОбновить базу репозиториев."

#: src/nhopkg.in:367
msgid " -b, --build\tCompiles and installs source package."
msgstr " -b, --build\tСобрать и установить пакет исходников."

#: src/nhopkg.in:368
msgid " -c, --create-source\tCreates source package from tarball."
msgstr " -c, --create-source\tСоздать пакет исходников из tarball'а."

#: src/nhopkg.in:369
msgid " -d, --dios\tSame as -S, --super-install."
msgstr " -d, --dios\tТоже, что и -S, --super-install."

#: src/nhopkg.in:370
msgid " -e, --clean\tRemove cached data."
msgstr " -e, --clean\tУдалить закешированные данные."

#: src/nhopkg.in:371
msgid " -g, --create-repo\tCreates software repository on selected directory."
msgstr ""
" -g, --create-repo\tСоздать репозиторий программы в выбранной директории."

#: src/nhopkg.in:372
msgid " -i, --install\tInstalls binary package."
msgstr " -i, --install\tУстановить бинарный пакет."

#: src/nhopkg.in:373
msgid " -k, --check\tChecks if installed package is correct."
msgstr " -k, --check\tПроверить, корректен ли установленный пакет."

#: src/nhopkg.in:374
msgid " -l, --list\tLists installed packages."
msgstr " -l, --list\tВывести список установленных пакетов."

#: src/nhopkg.in:375
msgid " -n, --info\tShows package details."
msgstr " -n, --info\tПоказать описание пакета."

#: src/nhopkg.in:376
msgid " -r, --remove\tRemoves package installed by Nhopkg."
msgstr " -r, --remove\tУдалить пакет, установленный Nhopkg."

#: src/nhopkg.in:377
msgid " -s, --search\tSearch for package and its details."
msgstr " -s, --search\tИскать пакет по имени и описанию."

#: src/nhopkg.in:378
msgid " -t, --list-repo\tLists packages on repository and their details."
msgstr ""
" -t, --list-repo\tВывести список пакетов по репозиторию и их описаниям."

#: src/nhopkg.in:379
msgid " -u, --update-db\tUpdate Nhopkg database."
msgstr " -u, --update-db\tОбновить базу Nhopkg."

#: src/nhopkg.in:380
msgid " -w, --show\tShows package files."
msgstr " -w, --show\tПоказать файлы пакета."

#: src/nhopkg.in:381
msgid " -x, --update-shooters\tUpdates all system data and schemas."
msgstr " -x, --update-shooters\tОбновляет все системные данные и схемы."

#: src/nhopkg.in:382
msgid " -z, --tgz2nho\tConverts .tgz package to .nho package."
msgstr " -z, --tgz2nho\tПреобразовать пакет .tgz в пакет .nho."

#: src/nhopkg.in:383
msgid " --license-all\tShows complete Nhopkg license."
msgstr "--license-all\tПоказать полную лицензию Nhopkg."

#: src/nhopkg.in:384
msgid " --license\tShows Nhopkg license."
msgstr " --license\tПоказать лицензию Nhopkg."

#: src/nhopkg.in:385
msgid " --version\tShows Nhopkg version."
msgstr " --version\tПоказать версию Nhopkg."

#: src/nhopkg.in:386
msgid " -h, --help\tShows this help page."
msgstr " -h, --help\tПоказать помощь по Nhopkg."

#: src/nhopkg.in:388
msgid "Options:"
msgstr "Опции:"

#: src/nhopkg.in:389
msgid " --force-check-arch\tForce check architecture."
msgstr " --force-check-arch\tПринудительная проверка архитектуры."

#: src/nhopkg.in:390
msgid " --force-check-deps\tForce check dependencies."
msgstr " --force-check-deps\tПринудительная проверка зависимостей."

#: src/nhopkg.in:391
msgid " --force-check-sha256\tForce check SHA256 Sum."
msgstr " --force-check-sha256\tПринудительная проверка SHA256."

#: src/nhopkg.in:392
msgid " --no-check-arch\tDon't check architecture."
msgstr " --no-check-arch\tНе проверять архитектуру."

#: src/nhopkg.in:393
msgid " --no-check-deps\tDon't check dependencies."
msgstr " --no-check-deps\tНе проверять зависимости."

#: src/nhopkg.in:394
msgid " --no-check-sha256\tDon't check SHA256 Sum."
msgstr " --no-check-sha256\tНе проверять сумму SHA256."

#: src/nhopkg.in:395
msgid " --purge\tRemove inverse dependencies of package."
msgstr " --purge\tУдалить обратные зависимости пакета."

#: src/nhopkg.in:396
msgid " --root\tSets install root."
msgstr " --root\tУстанавливает папку установки."

#: src/nhopkg.in:397
msgid " -R, --recursive\tNhopkg works recursively."
msgstr " -R, --recursive\tNhopkg работает рекурсивно."

#: src/nhopkg.in:398
msgid " -o, --output\tWrite an output log."
msgstr " -o, --output\tПисать в выходной журнал."

#: src/nhopkg.in:399
msgid " -v, --verbose\tVerbose operations."
msgstr " -v, --verbose\tБолтливый режим."

#: src/nhopkg.in:400
msgid " --\tStop parssing arguments."
msgstr " --\tКонец передачи аргументов."

#: src/nhopkg.in:416
msgid "Nhopkg is already in use..."
msgstr "Nhopkg уже работает..."

#: src/nhopkg.in:421
msgid "Exist an older lock file, but seems that is not in use..."
msgstr "Есть старый файл блокировки, но, похоже, он не задействован..."

#: src/nhopkg.in:427
msgid " * You can't rewrite lock file."
msgstr " * Вы не может заменить файл блокировки."

#: src/nhopkg.in:470
msgid "To run Nhopkg you need have root powers."
msgstr "Чтобы выполнить Nhopkg вам нужна сила root'а."

