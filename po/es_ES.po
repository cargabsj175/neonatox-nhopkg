#: ../src/nhopkg.in:203
msgid "Commands:"
msgstr "Comandos:"

#: ../src/nhopkg.in:204
msgid " -i, --install\tInstalls binary package."
msgstr " -i, --install\tInstala paquete binario."

#: ../src/nhopkg.in:205
msgid " -S, --super-install\tInstalls binary package form repository."
msgstr " -S, --super-install\tInstala paquete binario desde repositorios."

#: ../src/nhopkg.in:206
msgid " -d, --dios\tSame as -S, --super-install."
msgstr " -d, --dios\tIgual que -S, --super-install."

#: ../src/nhopkg.in:207
msgid " -r, --remove\tRemoves package installed by Nhopkg."
msgstr " -r, --remove\tElimina paquetes instalados por Nhopkg."

#: ../src/nhopkg.in:208
msgid " -B, --backup\tCreates binary package from installed package."
msgstr " -B, --backup\tCrea un paquete binario desde otro instalado."

#: ../src/nhopkg.in:209
msgid " -b, --build\tCompiles and installs source package."
msgstr " -b, --build\tCompila e instala un paquete fuente."

#: ../src/nhopkg.in:210
msgid " -k, --check\tChecks if installed package is correct."
msgstr " -k, --check\tComprueba si un paquete instalado es correcto."

#: ../src/nhopkg.in:211
msgid " -c, --create-source\tCreates source package from tarball."
msgstr " -c, --create-source\tCrea un paquete fuente desde un tarball."

#: ../src/nhopkg.in:212
msgid " -A, --auto-package\tCreates automatically source package from tarball."
msgstr " -A, --auto-package\tCrea automáticamente un paquete fuente desde un tarball."

#: ../src/nhopkg.in:213
msgid " -z, --tgz2nho\tConverts .tgz package to .nho package."
msgstr " -z, --tgz2nho\tConvierte un paquete .tgz a uno .nho."

#: ../src/nhopkg.in:214
msgid " -g, --create-repo\tCreates software repository on selected directory."
msgstr " -g, --create-repo\tCrea un repositorio de software en el directorio especificado."

#: ../src/nhopkg.in:215
msgid " -U, --update\tUpdate repository database."
msgstr " -U, --update\tActualiza la base de datos del repositorio."

#: ../src/nhopkg.in:216
msgid " -e, --clean\tRemove cached data."
msgstr " -e, --clean\tLimpia la cache."

#: ../src/nhopkg.in:217
msgid " -s, --search\tSearch for package and its details."
msgstr " -s, --search\tBusca un paquete y sus detalles."

#: ../src/nhopkg.in:218
msgid " -t, --list-repo\tLists packages on repository and their details."
msgstr " -t, --list-repo\tLista los paquetes en el repositorio y sus detalles."

#: ../src/nhopkg.in:219
msgid " -l, --list\tLists installed packages."
msgstr " -l, --list\tLista los paquetes instalados."

#: ../src/nhopkg.in:220
msgid " -n, --info\tShows package details."
msgstr " -n, --info\tMuestra los detalles de un paquete."

#: ../src/nhopkg.in:221
msgid " -w, --show\tShows package files."
msgstr " -w, --show\tMuestra los archivos de un paquete."

#: ../src/nhopkg.in:222
msgid " -u, --update-db\tUpdate Nhopkg database."
msgstr " -u, --update-db\tActualiza la base de datos de Nhopkg."

#: ../src/nhopkg.in:223
msgid " --license\tShows Nhopkg license."
msgstr " --license\tMuestra la licencia de Nhopkg."

#: ../src/nhopkg.in:224
msgid " --license-all\tShows complete Nhopkg license."
msgstr " --license-all\tMuestra la licencia completa de Nhopkg."

#: ../src/nhopkg.in:225
msgid " --version\tShows Nhopkg version."
msgstr " --version\tMuestra la versión de Nhopkg."

#: ../src/nhopkg.in:226
msgid " -h, --help\tShows Nhopkg help pages."
msgstr " -h, --help\tMuestra la ayuda de nhopkg."

#: ../src/nhopkg.in:228
msgid "Options:"
msgstr "Opciones:"

#: ../src/nhopkg.in:229
msgid " -R, --recursive\tNhopkg works recursively."
msgstr " -R, --recursive\tNhopkg trabaja de forma recursiva."

#: ../src/nhopkg.in:230
msgid " --root\tSets install root."
msgstr " --root\tSelecciona el directorio de instalación."

#: ../src/nhopkg.in:231
msgid " --no-check-deps\tDon't check dependencies."
msgstr " --no-check-deps\tNo comprueba dependencias."

#: ../src/nhopkg.in:232
msgid " --no-check-arch\tDon't check architecture."
msgstr " --no-check-arch\tNo comprueba la arquitectura."

#: ../src/nhopkg.in:233
msgid " --no-check-md5\tDon't check MD5 Sum."
msgstr " --no-check-md5\tNo comprueba la suma MD5."

#: ../src/nhopkg.in:234
msgid " --force-check-deps\tForce check dependencies."
msgstr " --force-check-deps\tFuerza la comprobación de dependencias."

#: ../src/nhopkg.in:235
msgid " --force-check-arch\tForce check architecture."
msgstr " --force-check-arch\tFuerza la comprobación de la arquitectura."

#: ../src/nhopkg.in:236
msgid " --force-check-md5\tForce check MD5 Sum."
msgstr " --force-check-md5\tFuerza la comprobación de la suma MD5."

#: ../src/nhopkg.in:237
msgid " --purge\tRemove inverse dependencies of package."
msgstr " --purge\tElimina las dependencias inversas del paquete."

#: ../src/nhopkg.in:238
msgid " -o, --output\tWrite an output log."
msgstr " -o, --output\tEscribe un log de la salida."

#: ../src/nhopkg.in:239
msgid " -v, --verbose\tVerbose operations."
msgstr " -v, --verbose\tOperaciones verbosas."

#: ../src/nhopkg.in:240
msgid " --\tStop parssing arguments."
msgstr " --\tNo obtiene más argumentos."

